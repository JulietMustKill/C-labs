Лаб-5(5в)1
#include <stdio.h>
#include <math.h>
int main()
{
    float x, y, res;
    printf("Число-");
    scanf("%f", &x);
    printf("Степень-");
    scanf("%f", &y);
    if(y>0)
    {res = pow(x, y);
    printf("res-> %.1f", res);}
    else{
        res = pow(1/x, -y);
        printf("res-> %.5f", res);
    }
    return 0;
}

